<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-lazy="false">
	<class name="com.erp.china.demo.model.System" table="tsystem">
		<id	name="systemId" type="java.lang.String" column="system_id" length="25" />
		<property name="systemVersion" type="java.lang.String" column="system_version" not-null="true" length="50" />
		<property name="lastModifiedDate" type="java.util.Date" column="system_last_modified_time" not-null="true" />
	</class>
</hibernate-mapping>